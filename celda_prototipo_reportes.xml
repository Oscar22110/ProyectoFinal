<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"


    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:tint="@color/primary_text"
            app:srcCompat="@drawable/reportes" />

        <TextView
            android:id="@+id/txtNumReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1 "
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txtEmpleReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Graciela Perez"
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Small" />

        <TextView
            android:id="@+id/txtCalidadReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Buena"
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Small" />

        <TextView
            android:id="@+id/txtIntensiReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="123.67"
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Small" />

        <TextView
            android:id="@+id/txtCxqReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="89.87"
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Small" />

        <TextView
            android:id="@+id/txtVeloReport"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="123.56"
            android:textColor="@color/primary_text"
            android:textAppearance="@style/TextAppearance.AppCompat.Small" />
    </LinearLayout>
</LinearLayout>
